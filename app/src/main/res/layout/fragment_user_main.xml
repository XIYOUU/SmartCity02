<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FAF6F6"
    tools:context=".user.UserMainFragment">

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:background="#FFFFFF"
            android:layout_width="match_parent"
            android:layout_height="130dp">
            <ImageView
                android:id="@+id/pic"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/user"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                ></ImageView>
            <TextView
                android:id="@+id/name"
                android:layout_toRightOf="@+id/pic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="user"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                ></TextView>
        </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_personinfo"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:orientation="horizontal"
        android:background="#FFFFFF"
        android:layout_marginTop="30dp"
        >
        <TextView
            android:id="@+id/tv_information"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="个人信息"
            android:textSize="30dp"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            ></TextView>
        <ImageView
            android:id="@+id/iv_enter1"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:src="@drawable/enter"
            android:layout_alignParentRight="true"
            >
        </ImageView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_orderList"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:orientation="horizontal"
        android:background="#FFFFFF"
        >
        <TextView
            android:id="@+id/tv_orederList"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="订单列表"
            android:textSize="30dp"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            ></TextView>
        <ImageView
            android:id="@+id/iv_enter2"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:src="@drawable/enter"
            android:layout_alignParentRight="true"
            >
        </ImageView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_changePwd"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:orientation="horizontal"
        android:background="#FFFFFF"
        >
        <TextView
            android:id="@+id/tv_changePwd"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="修改密码"
            android:textSize="30dp"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            ></TextView>
        <ImageView
            android:id="@+id/iv_enter3"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:src="@drawable/enter"
            android:layout_alignParentRight="true"
            >
        </ImageView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_feedback"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:orientation="horizontal"
        android:background="#FFFFFF"
        >
        <TextView
            android:id="@+id/tv_feedback"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="意见反馈"
            android:textSize="30dp"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            ></TextView>
        <ImageView
            android:id="@+id/iv_enter4"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:src="@drawable/enter"
            android:layout_alignParentRight="true"
            >
        </ImageView>
    </RelativeLayout>

    <TextView
        android:id="@+id/un_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="退出"
        android:textSize="30dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="#FFFFFF"
        android:layout_marginTop="40dp"
        ></TextView>
